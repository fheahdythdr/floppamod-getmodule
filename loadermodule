local function hget(filename)
    return loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/fheahdythdr/floppamod-getmodule/main/"..filename))()
end

hget("games")

if table.find(backroom, cg) then
    hget("backroom")
end
if table.find(cotc, cg) then
    hget("cotc")
end
if table.find(pf, cg) then
    hget("pfesp")
end
